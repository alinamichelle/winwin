<h1>This is <%= @user.name%>'s profile</h1>
<h3>Info</h3>
  <p><%= @user.avatar_url %></p>
  <p><%= @user.name %></p>
  <p>Birthday:</p>
  <p><%= @user.birthday %></p>
  <p>Location:</p>
  <p><%= @user.location %></p>
  <p>Tagline:</p>
  <p><%= @user.tagline %></p>
  <h5>Biography</h5>
  <p><%= @user.biography %></p>

<h3>Expertise</h3>
  <ul>
    <% @user.expertises.each do |expertise| %>
    <li><%= expertise.topic.name %></li>
      <li><%= expertise.experience %></li>
    <% end %>
  </ul>






<%= link_to "Messages", "#testMessage", "data-toggle" => "modal", class:"btn btn-primary" %>
<%= render "shared/modal_for_message" %>


